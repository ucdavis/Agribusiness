@model Agribusiness.Import.Models.View.FirmViewModel

@{
    ViewBag.Title = "Index";
}


<h2>Index</h2>

<div>
    <ul>
    <li>
        <em>Imported Already?</em> @(Model.AlreadyImported ? "Yes" : "No")    
    </li>
    <li>
        <em># Read:</em> @Model.Firms.Count
    </li>
    <li>
        <em># Error:</em> @Model.Errors.Count
    </li>
    </ul>
</div>

@Html.Partial("_Error", Model.Errors)


@model Agribusiness.Import.Models.View.ContactViewModel

@{
    ViewBag.Title = "ArchiveContacts";
}

<h2>ArchiveContacts</h2>

<div>
    <ul>
    <li>
        <em>Imported Already?</em> @(Model.AlreadyImported ? "Yes" : "No")    
    </li>
    <li>
        <em># Read:</em> @Model.Contacts.Count
    </li>
    <li>
        <em># Error:</em> @Model.Errors.Count
    </li>
    </ul>
</div>

@Html.Partial("_Error", Model.Errors)

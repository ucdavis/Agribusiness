@model SeminarViewModel

@{
    ViewBag.Title = "Create Seminar";
    ViewBag.SubPageTitle = "Create Seminar";
    ViewBag.Restrict = false;
}

@section ScriptContent
{
    @Html.Partial("_DateTimePickerScripts", Model.Seminar)
    @Html.Partial("_tinymce", new TinyMceViewModel())
}

@Html.ValidationSummary("Please correct the following errors.")
@using (Html.BeginForm("Create", "Seminar", FormMethod.Post, new { @class = "form-horizontal", enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    @Html.Partial("_SeminarForm")    
}



